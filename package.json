{
  "name": "custom-javaScript-for-websites-2",
  "version": "3.0.1",
  "description": "Run custom JavaScript on any website.",
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "build": "node utils/build.js",
    "deploy": "NODE_ENV=production yarn build && yarn zip",
    "zip": "zip -X --compression-method deflate -r build.zip build",
    "lint": "standard",
    "lintfix": "yarn lint -- --fix",
    "start": "node utils/webserver.js",
    "test": "yarn lint && yarn build"
  },
  "dependencies": {
    "chrome-extension-async": "^3.2.4",
    "clean-webpack-plugin": "^0.1.17",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.18",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-ace": "^5.4.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "typeface-roboto": "^0.0.43"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.9.1",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/lib"
    ],
    "globals": [
      "ace",
      "chrome"
    ]
  }
}
